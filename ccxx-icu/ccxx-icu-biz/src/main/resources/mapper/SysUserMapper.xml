<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.pig4cloud.pigx.ccxxicu.mapper.SysUserMapper">

  <resultMap id="sysUserMap" type="com.pig4cloud.pigx.ccxxicu.api.entity.SysUser">
                  <id property="userId" column="user_id"/>
